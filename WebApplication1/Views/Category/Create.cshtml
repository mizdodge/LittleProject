}
<div class="">
    <div class="row"><h2 class="">Register</h2></div>
    <div class="row">
        <form class="form-auth-small" asp-controller="Category" asp-action="Create" id="form-data">
            @Html.AntiForgeryToken()
            <div class="col-md-6 row">
                <div class="col-md-4">Category Id </div>

                <div class="col-md-8">
                    @Html.TextBoxFor(model => data.CategoryID, new { @name="CategoryName", @class = "form-control", @required = "required", id = "CategoryID", @placeholder = "CategoryID", @disabled="disabled" })
                    @Html.HiddenFor(x => data.CategoryID)
                </div>
                <br /><br />
                <div class="col-md-4">Category Name </div>

                <div class="col-md-8">
                    @Html.TextBoxFor(model => CategoryName, new { @name="CategoryName", @class = "form-control", @required = "required", id = "CategoryName", @placeholder = "Category Name" })
                </div>
                <br /><br />
                <br /><br />
                <div class="col-md-4" onclick="backBtn(this)"><button type="button" class="btn btn-success btn-lg btn-block">back</button></div>
                <div class="col-md-8"><button type="submit" class="btn btn-primary btn-lg btn-block" @* onclick="submitBtn(this)" *@>SUBMIT</button></div>
            </div>
        </form>
    </div>
